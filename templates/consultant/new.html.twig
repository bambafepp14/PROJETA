{% extends 'base.html.twig' %}

{% block title %}Ajout consultant{% endblock %}

{% block body %}
    {{ form_start(form) }}
    {{ form_errors(form) }}

    {{ form_row(form.nom, {'label_attr': {'class': 'form-control'}}) }}
    {{ form_row(form.prenom, {'label_attr': {'class': 'form-control'}}) }}
    {{ form_row(form.email, {'label_attr': {'class': 'form-control'}}) }}
    {{ form_row(form.password, {'label_attr': {'class': 'form-control'}}) }}

    {{ form_row(form.envoyer, { 'label': 'Envoyer' }) }}
    {{ form_end(form) }}
{% endblock %}
